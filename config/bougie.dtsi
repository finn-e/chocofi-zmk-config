TCOMBO(L,          &as L,              &kp ASTERISK,          &kp CARET,             &kp F12,                BLM BLI,                           BRM BRI)
TCOMBO(Y,          &as Y,              &kp HASH,              &kp LS(RA(SEMI)),      &kp F21,                TLR BLM,                           TRR BRM)
TCOMBO(G,          &as G,              &kp DOLLAR,            &kp RA(N5),            &kp F18,                BLR TLM,                           BRR TRM)
TCOMBO(W,          &as W,              &kp PERCENT,           &kp RA(SEMI),          &kp F20,                TLR BLI,                           TRR BRI)
TCOMBO(M,          &as M,              &kp SLASH,             &kp BSLH,              &kp F16,                BLR TLI,                           BRR TRI)
TCOMBO(D,          &as D,              &kp PRINTSCREEN,       &kp LS(PRINTSCREEN),   &kp F13,                TLM BLI,                           TRM BRI)
TCOMBO(F,          &as F,              &sk_lshift_lock,       &sk_lshift_lock,       &kp F17,                BLM TLI,                           BRM TRI)
TCOMBO(APOS,       &kp APOS,           &kp DQT,               &kp GRAVE,             &tqt,                   TLR BLR TLM,                       TRR BRR TRM)
TCOMBO(COMMA,      &kp COMMA,          &kp QMARK,             &kp SEMI,              &kp RA(SLASH),          TLR BLR TLI,                       TRR BRM TRI)
TCOMBO(DASH,       &kp MINUS,          &kp UNDER,             &kp TILDE,             &kp LS(RA(SLASH)),      TLR BLR BLM,                       TRR BRR BRM)
TCOMBO(DOT,        &kp DOT,            &kp EXCL,              &kp COLON,             &kp RA(N1),             TLR BLR BLI,                       TRR BRM BRI)
TCOMBO(P,          &as P,              &kp PRINTSCREEN,       &kp LS(PRINTSCREEN),   &kp F19,                TLM BLM TLR,                       TRM BRM TRR)
TCOMBO(B,          &as B,              &kp LG(W),             &kp LG(G),             &kp F22,                TLM BLM BLR,                       TRM BRM BRR)
TCOMBO(Z,          &as Z,              &kp C_BRI_UP,          &kp C_BRI_DN,          &kp C_BKLT_TOG,         TLM BLM TLI,                       TRM BRM BRI)
TCOMBO(X,          &as X,              &kp C_VOL_UP,          &kp C_VOL_DN,          &kp C_MUTE,             TLM BLM BLI,                       TRM BRM TRI)
TCOMBO(Q,          &as Q,              &none,                 &none,                 &kp F1,                 TLI BLI TLR,                       TRI BRI TRR)
TCOMBO(J,          &as J,              &kp C_NEXT,            &kp C_PREV,            &kp C_PP,               TLI BLI BLR,                       TRI BRI BRR)
TCOMBO(V,          &as V,              &kp LC(LA(T)),         &none,                 &kp F23,                TLI BLI TLM,                       TRI BRI TRM)
TCOMBO(K,          &as K,              &kp LA(SPACE),         &kp LGUI,              &kp F24,                TLI BLI BLM,                       TRI BRI BRM)
TCOMBO(BT,         &sl 5,              &none,                 &none,                 &none,                  TLR BLR TLM BLM,                   TRR BRR TRM BRM)
TCOMBO(NAVLOCK,    &sticky_navlock,    &none,                 &none,                 &none,                  TLR BLR TLI BLI,                   TRR BRR TRI BRI)
TCOMBO(NUMLOCK,    &sticky_numlock,    &sk_lshift_lock,       &none,                 &bt BT_CLR,             TLM BLM TLI BLI,                   TRM BRM TRI BRI)
TCOMBO(RET,        &kp RET,            &kp LS(RET),           &kp LS(RET),           &kp LS(RET),            TLR TLM TLI,                       TRR TRM TRI)
TCOMBO(PASTE,      &kp LC(V),          &kp LS(LC(V)),         &none,                 &none,                  TLR TLM BLI,                       TRR TRM BRI)
TCOMBO(COPY,       &kp LC(C),          &kp LS(LC(C)),         &none,                 &none,                  TLR BLM TLI,                       TRR BRM TRI)
TCOMBO(CUT,        &kp LC(X),          &kp LS(LC(X)),         &none,                 &none,                  BLR TLM TLI,                       BRR TRM TRI)
TCOMBO(UNDO,       &kp LC(Z),          &kp LS(LC(Z)),         &none,                 &none,                  TLR BLM BLI,                       TRR BRM BRI)
TCOMBO(SAVE,       &kp LC(S),          &kp LG(G),             &kp LG(W),             &kp LS(LC(S)),          BLR TLM BLI,                       BRR TRM BRI)
TCOMBO(REDO,       &kp LS(LC(Z)),      &kp LC(Z),             &none,                 &none,                  BLR BLM TLI,                       BRR BRM TLI)
TCOMBO(ESC,        &kp ESC,            &none,                 &none,                 &none,                  BLR BLM BLI,                       BRR BRM BRI)
TCOMBO(RESET,      &sys_reset,         &none,                 &none,                 &none,                  TLR BLR TLM BLM TLI BLI,           TRR BRR TRM BRM TRI BRI)
TCOMBO(BOOTLOADER, &bootloader,        &none,                 &none,                 &none,                  TLP BLP TLR BLR TLM BLM TLI BLI,   TRP BRP TRR BRR TRM BRM TRI BRI)
